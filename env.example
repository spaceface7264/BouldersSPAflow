# API Configuration
VITE_BRP_BASE_URL=https://api.boulders.dk

# API Authentication Token
# Get this from Postman documentation: https://documenter.getpostman.com/view/6552350/2sB3QNq9Fv
# Required for accessing the Join Boulders API
VITE_API_AUTH_TOKEN=your_api_token_here

# Environment
NODE_ENV=development

# Google Tag Manager Container ID
# Replace GTM-XXXXXXX in index.html with your actual GTM container ID
# Format: GTM-XXXXXXX (e.g., GTM-ABC123)
VITE_GTM_CONTAINER_ID=GTM-XXXXXXX
